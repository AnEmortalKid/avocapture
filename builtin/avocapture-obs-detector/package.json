{
  "name": "avocapture-obs-detector",
  "version": "0.2.0",
  "description": "A replay detector for avocapture that uses obs events",
  "main": "extension.js",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "keywords": [
    "avocapture",
    "evocati"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "obs-websocket-js": "^5.0.0"
  },
  "avocapture": {
    "type": "detector",
    "display": "OBS Replay Detector",
    "settings": {
      "defaults": {
        "serverPort": 4455,
        "serverPassword": null,
        "reconnectIntervalSeconds": 5
      },
      "view": {
        "entry": "index.html"
      }
    }
  },
  "devDependencies": {
    "jest": "^28.1.2"
  }
}
